{
  "name": "Arizona JS",
  "type": "module",
  "scripts": {
    "build": "node ./esbuild.config.mjs",
    "format": "prettier --write \"./*.{js,mjs}\" \"./assets/js/*.mjs\" \"**/*.json\" \"**/*.yml\" --config prettier.config.mjs --ignore-path .prettierignore",
    "format:check": "prettier --check \"./*.{js,mjs}\" \"./assets/js/*.mjs\" \"**/*.json\" \"**/*.yml\" --config prettier.config.mjs --ignore-path .prettierignore",
    "lint": "eslint eslint.config.mjs --fix",
    "lint:check": "eslint eslint.config.mjs",
    "format:lint": "npm run format && npm run lint",
    "test": "npm install && NODE_OPTIONS='$NODE_OPTIONS --experimental-vm-modules' npx jest",
    "ci": "npm install && npm run format:check && npm run lint:check"
  },
  "devDependencies": {
    "@babel/preset-env": "7.26.9",
    "@eslint/js": "9.22.0",
    "babel-jest": "29.7.0",
    "esbuild": "0.25.2",
    "eslint": "9.22.0",
    "globals": "16.0.0",
    "jest": "29.7.0",
    "prettier": "3.5.3",
    "uglify-js": "3.19.3"
  },
  "dependencies": {
    "morphdom": "2.7.4"
  },
  "overrides": {
    "glob": "^10.0.0"
  }
}
